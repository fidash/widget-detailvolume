<?xml version='1.0' encoding='UTF-8'?>
<widget xmlns="http://wirecloud.conwet.fi.upm.es/ns/macdescription/1" vendor="CoNWeT" name="openstack-volumedetails-widget" version="0.5.1">
  <details>
    <title>OpenStack Volume Details Widget</title>
    <email>bgrana@conwet.com</email>
    <image>images/catalogue.png</image>
    <smartphoneimage>images/catalogueSmartphone.png</smartphoneimage>
    <description>Widget that shows the details of an openStack volume</description>
    <authors>bgrana</authors>
  </details>
  <requirements>
    <feature name="StyledElements" />
  </requirements>
  <wiring>
    <inputendpoint name="volume_id" type="text" label="Volume ID" description="Receives volume ID and OpenStack service token." friendcode="volume_id"/>
  </wiring>
  <contents src="index.html" useplatformstyle="true"/>
  <rendering height="27" width="11"/>
</widget>
